{
  "name": "rd-dashboard-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "bunx biome lint . && bunx biome format --check . && bunx biome check .",
    "preview": "vite preview",
    "generate-supabase-types": "bunx supabase gen types typescript --project-id vrgjjmrqvvpmoozydmqp > src/database/database_types.ts",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "deploy": "npm run build && touch dist/.nojekyll && cp dist/index.html dist/404.html"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "bunx biome format --write",
      "bunx biome lint --apply"
    ],
    "**/*": [
      "bash -c 'bunx tsc --noEmit --skipLibCheck --project tsconfig.json'"
    ]
  },
  "dependencies": {
    "@dnd-kit/accessibility": "^3.1.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-date-pickers": "^7.28.3",
    "@react-google-maps/api": "^2.20.6",
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/supabase-js": "^2.48.1",
    "@toolpad/core": "^0.12.0",
    "@types/uuid": "^10.0.0",
    "@vis.gl/react-google-maps": "^1.5.2",
    "date-fns": "^4.0.0-beta.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0",
    "stripe": "^18.1.0",
    "tailwindcss": "^3.4.13",
    "use-places-autocomplete": "^4.0.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0-rc.1",
    "@types/react-dom": "^19.0.0-rc.1",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^3.1.2",
    "autoprefixer": "^10.4.20",
    "biome": "^0.3.3",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.47",
    "typescript": "^5.7.3",
    "vite": "^5.4.8",
    "vitest": "^3.1.2"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
